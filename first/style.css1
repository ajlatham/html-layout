.container {
	width: 100%;
	height: 100vh;
	position: relative;
}

.header {
	width: 100%;
	height: 20vh;
	background: gray;

}

.left {
	width: 20%;
	height: 80vh;
	background: black;
	float: left;
	
}
.right {
	width: 20%;
	height: 80vh;
	background: black;
	float: right;
	
}
.footer {
	width: 60%;
	height: 10vh;
	background: gray;
	margin-left: 20%;
	position: absolute;
	bottom: 0px;
	
}